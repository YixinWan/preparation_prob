opacity == 1 and mouse_startpoint_time.getTime() &gt;= random_origin_duration:&amp;#10;        beep_started = True&amp;#10;        beep_time.reset()  # 重置beep计时器&amp;#10;        &amp;#10;        # 在(750 - preparation)ms 后显示目标&amp;#10;    if beep_started and beep_time.getTime() &gt;= target_show_delay and not target_show:&amp;#10;        target_familiarization.opacity = 1&amp;#10;        target_show = True  # 标记目标已显示&amp;#10;        target_onset_time_sys = core.monotonicClock.getTime()&amp;#10;        target_show_time.reset()  # 重置目标显示计时器&amp;#10;            &amp;#10;    # 如果两帧之间的鼠标移动距离大于 0.005 且目标已经显示，则运动开始&amp;#10;    if mouseDist &gt; 0.1 and target_show:&amp;#10;        move_start = True&amp;#10;        RT = target_show_time.getTime()&amp;#10;        diff_ppt_rt = RT - ppt*0.001&amp;#10;        move_start_time.reset()&amp;#10;        move_onset_time_sys = core.monotonicClock.getTime()&amp;#10;    else:&amp;#10;        move_start = False&amp;#10;            &amp;#10;# 当beep开始播放后&amp;#10;if beep_started:&amp;#10;    # 播放 beep 声音&amp;#10;    if beep_count &lt; 4 and beep_time.getTime() &gt;= beep_count * beep_interval:&amp;#10;        beep.play()&amp;#10;        beep_count += 1&amp;#10;        &amp;#10;## move start&amp;#10;if move_start == 1:&amp;#10;    movement_frames.append(mouse_familiarization.getPos())&amp;#10;    movement_frames_times_sys.append(core.monotonicClock.getTime())&amp;#10;    #target_baseline.opacity = 0&amp;#10;    mouse_startpoint_ring_familiarization.opacity = 0&amp;#10;    ring_familiarization.opacity = 0&amp;#10;    # move end&amp;#10;    #if mouseDist &lt; 0.01 or move_start_time.getTime() &gt; 1.2:&amp;#10;    # If the movement distance between two frames is less than 0.02, the movement ends&amp;#10;    # 若两帧运动距离小于0.002，运动结束&amp;#10;    if mouseDist &lt; 0.01 and move_start_time.getTime() &gt; 0.3: &amp;#10;        move_start = 0&amp;#10;        feedback_start = 1&amp;#10;        move_end_time.reset()&amp;#10;        move_end_time_sys = core.monotonicClock.getTime()&amp;#10;    else:&amp;#10;        move_start = 1&amp;#10;        feedback_start = 0&amp;#10;        &amp;#10;&amp;#10;## feedback start&amp;#10;if feedback_start == 1:&amp;#10;    if move_end_time.getTime() &gt;= feedback_delay:  # feedback delay&amp;#10;        if not feedback_position_set:&amp;#10;            # Conversion of feedback position 反馈位置的转换&amp;#10;            [theta_mouse, rho_mouse] = cart2pol(x_mouse, y_mouse)&amp;#10;            [feedback_x, feedback_y] = pol2cart(theta_mouse, target_radius)&amp;#10;            &amp;#10;            feedback_position_set = True  # Set the flag to True&amp;#10;            &amp;#10;        feedback_ring_familiarization.opacity = 1&amp;#10;        target_familiarization.opacity = 1&amp;#10;        &amp;#10;    else:&amp;#10;        feedback_ring_familiarization.opacity = 0&amp;#10;        #target_baseline.opacity = 0&amp;#10;        feedback_start_time.reset()&amp;#10;    if feedback_ring_familiarization.opacity == 1 and feedback_start_time.getTime() &gt;= feedback_duration:  # feedback duration设为1s&amp;#10;        feedback_ring_familiarization.opacity = 0&amp;#10;        target_familiarization.opacity = 0&amp;#10;        blank_start = 1&amp;#10;    else:&amp;#10;        blank_start_time.reset()&amp;#10;            &amp;#10;     &amp;#10;else:&amp;#10;    feedback_ring_familiarization.opacity = 0&amp;#10;    &amp;#10;&amp;#10;if blank_start == 1 and blank_start_time.getTime() &gt;= blank_screen:&amp;#10;    continueRoutine = False&amp;#10;            &amp;#10;                " valType="extendedCode" updates="constant" name="Each Frame"/>
