        <Param val="from psychopy.tools.coordinatetools import cart2pol&amp;#10;from psychopy.tools.coordinatetools import pol2cart&amp;#10;from psychopy import sound&amp;#10;from psychopy.sound import Sound&amp;#10;import random&amp;#10;from time import sleep, time&amp;#10;&amp;#10;# 初始化试验计数&amp;#10;trial_number = 0  &amp;#10;&amp;#10;# Initialize beep sound&amp;#10;beep = sound.Sound(800, secs=0.005) # each beep last 5ms&amp;#10;&amp;#10;# Calculate Euclidean distance 计算欧氏距离 &amp;#10;def euclidean_dist(vec1, vec2):&amp;#10;    return sqrt((vec1[0] - vec2[0])**2 + (vec1[1] - vec2[1])**2)&amp;#10;# origin hold durations&amp;#10;random_origin_durations = [1.0, 1.2, 1.4, 1.6]   &amp;#10;# might change depending on which computer/screen is being used&amp;#10;target_radius = 8 # 12 # 10&amp;#10;# rotation angle&amp;#10;angle = 30.0&amp;#10;# group info: NoDelay=0, LongDelay=1&amp;#10;group = expInfo['group']&amp;#10;if group == '0':&amp;#10;    feedback_delay = 0.03&amp;#10;    blank_screen = 1.45&amp;#10;elif group == '1':&amp;#10;    feedback_delay = 1.2&amp;#10;    blank_screen = 0.3&amp;#10;&amp;#10;# feedback duration 反馈显示的时长&amp;#10;feedback_duration = 1.0&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
